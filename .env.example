# Keyway API
# URL of the backend API
NEXT_PUBLIC_KEYWAY_API_URL=http://localhost:3000

# PostHog Analytics

# Client-side tracking (pageviews, custom events, user journey)
# Get your project API key from PostHog dashboard
NEXT_PUBLIC_POSTHOG_KEY=
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Server-side tracking (badge analytics only)
POSTHOG_SERVER_API_KEY=
POSTHOG_HOST=https://app.posthog.com

# Sentry Error Tracking
# Get your DSN from Sentry dashboard (Settings > Projects > [project] > Client Keys)
NEXT_PUBLIC_SENTRY_DSN=

# Crisp Live Chat
# Get your Website ID from Crisp dashboard (Settings > Website Settings)
NEXT_PUBLIC_CRISP_WEBSITE_ID=
