{
  "name": "keyway-api",
  "version": "0.1.0",
  "description": "GitHub-native secrets manager API",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/db/migrate.ts"
  },
  "keywords": [
    "secrets",
    "github",
    "api",
    "encryption"
  ],
  "author": "Nicolas Ritouet",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.25.1",
    "@fastify/cors": "^8.4.2",
    "drizzle-orm": "^0.29.1",
    "postgres": "^3.4.3",
    "zod": "^3.22.4",
    "posthog-node": "^3.5.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "drizzle-kit": "^0.20.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
